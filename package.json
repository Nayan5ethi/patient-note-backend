{
  "name": "bitespeed",
  "version": "0.0.1",
  "description": "Bitespeed",
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -p ./",
    "build:clean": "rm -rf build tsconfig.tsbuildinfo && tsc -p ./",
    "start": "NODE_PATH='./build' node build/index.js",
    "pre-commit": "lint-staged && tsc && yarn test",
    "test": "jest --passWithNoTests --detectOpenHandles",
    "test:watch": "jest --watch",
    "format": "prettier --write 'src/**/*.{ts,js,json}'",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:sync": "prisma migrate dev",
    "prisma:create-migration": "prisma migrate dev --create-only",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "seed": "ts-node -r tsconfig-paths/register src/services/seed.service.ts",
    "seed:clear": "ts-node -r tsconfig-paths/register src/services/seed.service.ts --clear",
    "seed:validate": "ts-node -r tsconfig-paths/register src/services/seed.service.ts --validate",
    "eslint": "npx eslint . --ext .ts"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.4.1",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "prisma": "^6.4.1",
    "tesseract.js": "^6.0.1",
    "typescript": "^5.1.6",
    "uuid": "^11.1.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.3",
    "@types/node": "^22.13.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": "22"
  }
}
